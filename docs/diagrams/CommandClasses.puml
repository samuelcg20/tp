@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor LOGIC_COLOR_T4
skinparam classBackgroundColor LOGIC_COLOR

package Model {
Class HiddenModel #FFFFFF
}

package "Command Classes" {
Class "{abstract}\nCommand" as Command
Class "{abstract}\nXYZCommand" as XYZCommand
Class "XYZMemberCommand" as XYZMemberCommand
Class "XYZEventCommand" as XYZEventCommand
XYZCommand -up-|> Command
XYZMemberCommand -up-|> XYZCommand
XYZEventCommand -up-|> XYZCommand
Command .up.> CommandResult : <<create>>
Command .right.> Model
}
@enduml