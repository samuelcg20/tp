   @startuml
!include style.puml
skinparam ArrowFontStyle plain

actor User

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as Parser LOGIC_COLOR
participant "m:MarkCommand" as MarkCommand LOGIC_COLOR
end box

[-> LogicManager : execute("mark m/1 e/2")
activate LogicManager

LogicManager -> Parser : parseCommand("mark m/1 e/2")
activate Parser

create MarkCommand
Parser -> MarkCommand : new MarkCommand(memberIndex, eventIndex)
activate MarkCommand
MarkCommand --> Parser
deactivate MarkCommand

Parser --> LogicManager : MarkCommand
deactivate Parser

LogicManager -> MarkCommand : execute(model)
activate MarkCommand

MarkCommand --> LogicManager : CommandResult
deactivate MarkCommand

LogicManager --> User : CommandResult
deactivate LogicManager
@enduml
